{
  "ConnectionStrings": {
    "NemesisEuchreDb": "Server=(localdb)\\mssqllocaldb;Database=NemesisEuchre;Trusted_Connection=True;MultipleActiveResultSets=true"
  },
  "GameExecution": {
    "MaxDegreeOfParallelism": 8
  },
  "Persistence": {
    "BatchSize": 100,
    "MaxBatchSizeForChangeTracking": 50
  },
  "Logging": {
    "LogLevel": {
      "Default": "Critical",
      "NemesisEuchre.DataAccess": "Information"
    },
    "Console": {
      "FormatterName": "simple",
      "FormatterOptions": {
        "SingleLine": false,
        "TimestampFormat": "yyyy-MM-dd HH:mm:ss ",
        "IncludeScopes": true
      }
    }
  }
}
